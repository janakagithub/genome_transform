{   "ver": "1.0.0",

    "authors": [
        "janaka"
    ],
    "contact": "help@kbase.us",
    "visible": false,
    "categories": ["inactive","upload"],
    "widgets": {
        "input": null,
        "output": "kbaseReportView"
    },
    "parameters": [
        {
            "id" : "file_path",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [],
            "field_type" : "text",
            "text_options" : {
                "valid_ws_types" : [ ]
            }
        },
        {
            "id": "source",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "RefSeq" ],
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [
                    {
                      "value": "RefSeq",
                      "display": "RefSeq",
                      "id": "RefSeq",
                      "ui_name": "RefSeq"
                    },
                    {
                      "value": "Ensembl",
                      "display": "Ensembl",
                      "id": "Ensembl",
                      "ui_name": "Ensembl"
                    },
                    {
                      "value": "Other",
                      "display": "Other",
                      "id": "Other",
                      "ui_name": "Other"
                    }
                ]

            }
        },
        {
            "id" : "genome_name",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "" ],
            "field_type" : "text",
            "text_options" : {
                "valid_ws_types" : ["KBaseGenomes.Genome"],
                "is_output_name":true

            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "genome_transform",
            "method": "genbank_to_genome_GFU",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace"
                },
                {
                    "input_parameter": "genome_name",
                    "target_property": "genome_name"
                },
                {
                    "input_parameter": "file_path",
                    "target_property": "file_path"
                },
                {
                    "input_parameter": "source",
                    "target_property": "source"
                }
            ],
            "output_mapping": [
                {
                  "narrative_system_variable": "workspace",
                  "target_property": "ws"
                },
                {
                  "constant_value": "KBaseGenomes.Genome",
                  "target_property": "type"
                },
                {
                    "constant_value": "16",
                    "target_property": "report_window_line_height"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
